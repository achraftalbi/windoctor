<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">


    <!--
        put your sample data here, pay attention to order of import
    -->
    <changeSet id="20150926164808" author="MBoufnichel">
        <loadData encoding="UTF-8"
                  file="config/liquibase/windoctor-sampledata/structure.csv"
                  separator=";"
                  tableName="STRUCTURE">
            <column name="id" type="numeric"/>
            <column name="name" type="string"/>
        </loadData>
        <loadData encoding="UTF-8"
                  file="config/liquibase/windoctor-sampledata/mailtypes.csv"
                  separator=";"
                  tableName="mail_type">
            <column name="id" type="numeric"/>
            <column name="label" type="string"/>
            <column name="description" type="string"/>
            <column name="content" type="string"/>
        </loadData>

    </changeSet>
</databaseChangeLog>
